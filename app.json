{
    "expo": {
        "name": "Bir-kitap",
        "slug": "bir-kitap",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#005479",
            "boxShadow": "inset 0 4px 4px 0 rgba(0, 0, 0, 0.25)"
        },
        "assetBundlePatterns": ["**/*"],
        "plugins": [
            [
                "expo-build-properties",
                {
                    "android": {
                        "usesCleartextTraffic": true
                    }
                }
            ]
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "mobile.birkitap",
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "scheme": "book-crossing",
        "extra": {
            "eas": {
                "projectId": "7efb6519-6b5b-450d-8381-88ace2a66719"
            }
        }
    }
}
